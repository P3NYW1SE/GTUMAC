{
  "name": "gtu-live-monorepo",
  "private": true,
  "workspaces": ["server", "web"],
  "scripts": {
    "install:all": "npm install --prefix server && npm install --prefix web",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:web\"",
    "dev:server": "npm run dev --prefix server",
    "dev:web": "npm run dev --prefix web"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}